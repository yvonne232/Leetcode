# Rolling array for space optimization O(1), Time complexity O(n)

class Solution:
    def climbStairs(self, n: int, costs: List[int]) -> int:
        # dp[n] = min(dp[n-1]+(1)^2, dp[n-2]+(2)^2, dp[n-3]+(3)^2)
        v0, v1, v2 = 0, 0, 0
        for c in costs:
            v0, v1, v2 = v1, v2, min(v0+9, v1+4, v2+1)+c
        return v2

# At the end of the loop, v2 is always the DP value for the last step, so returning v2 returns dp[n].
